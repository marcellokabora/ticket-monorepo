{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": false,
        "experimentalDecorators": true,
        "module": "ESNext",
        "resolveJsonModule": true,
        "allowJs": true,
        "checkJs": true,
        "isolatedModules": true,
        "moduleDetection": "force",
        "noEmit": true,
        "types": [
            "vite/client"
        ]
    }
}